SELECT DISTINCT OBJECT_SCHEMA_NAME(ID)+'.'+OBJECT_NAME(ID) FROM SYS.SYSCOMMENTS 
WHERE TEXT LIKE '%TP_CUSTOMER_PREPAIDACCOUNTS%'
SELECT * FROM TOLLPLUS.INBOUNDTXNS_PROCESSTXNS_REPROCESS
ORDER BY 1 DESC

UPDATE REP
SET TXNTS=GETDATE()
,HEADTS=GETDATE()
,ISPROCESSED=0
--SELECT NOTE,*
FROM TOLLPLUS.INBOUNDTXNS_PROCESSTXNS_REPROCESS REP
WHERE REPROCESSID IN (
675
,674
) 
